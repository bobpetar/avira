<%= form_for @webcam do |f| %>
  <div class="form-group">
    <label>Image 1</label>
    <%= f.file_field :image1, class: "form-control" %>
  </div>

  <div class="form-group">
    <label>Image 2</label>
    <%= f.file_field :image2, class: "form-control" %>
  </div>

  <div class="form-group">
    <label>Image 3</label>
    <%= f.file_field :image3, class: "form-control" %>
  </div>

  <%= f.submit %>
<% end %>
